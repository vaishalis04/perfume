/* Style the container for influencers */
.container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    /* margin: 20px; */
    flex-wrap: wrap;
    white-space: no wrap;
}

/* Style each influencer card */
.influencer {
    flex-basis: calc(33.33% - 20px);
    margin-bottom: 20px;
    padding: 20px;
    background-color: #f5f5f5;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    text-align: center;
}

.influencer img {
    max-width: 100%;
    height: auto;
    border-radius: 50%;
    margin-bottom: 10px;
}

.influencer h2 {
    font-size: 24px;
    margin-bottom: 10px;
}

.influencer p {
    font-size: 16px;
    line-height: 1.5;
    margin-bottom: 10px;
}

.influencer a {
    text-decoration: none;
    color: #333;
    font-weight: bold;
}

.influencer a:hover {
    text-decoration: underline;
}
